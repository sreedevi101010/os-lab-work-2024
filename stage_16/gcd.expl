decl
	int gcd(int a, int b);
enddecl

int gcd(int a, int b)
{
decl
    int result;
enddecl
begin
    if(b==0) then
    	result = a;
    else
    	result=gcd(b,a%b);
    endif;
    return result;
end
}

int main()
{
decl
	int temp, a, b, result;
enddecl
begin
	temp = exposcall("Read" , -1, a);
	temp = exposcall("Read", -1, b);
	result = gcd(a,b);
	temp=exposcall("Write", -2, result);
	return 0;
end
}
